<!-- Home > Groups. -->
<script type='text/x-handlebars-template' id='home_groups'>
    <div class='bs-docs-section'>
        <h2 id='home-groups'>
            Groups List
        </h2>
        <p>
            Retrieve the list of available FAOSTAT groups. This function is used to generate the header
            of the main page containing the links to the Download and Browse sections for each group. The same
            function is also invoked to populate the drop-down sections of the main menu.
        </p>
        <div class='row'>
            <div class='col-lg-6'>
                <h4>Endpoint</h4>
                <code>
                    http://faostat3.fao.org/api/groups/
                </code>
            </div>
            <div class='col-lg-6'>
                &nbsp;
            </div>
        </div>
        <div class='row'>
            <div class='col-lg-6'>
                <h4>Inputs</h4>
                <table class='table table-bordered table-responsive table-striped table-hover table-condensed'>
                    {{> table_headers}}
                    <tbody>
                        {{> table_commons}}
                    </tbody>
                </table>
            </div>
            <div class='col-lg-6'>
                <h4>Input Example</h4>
                {{> home_groups_in}}
            </div>
        </div>
        <div class='row'>
            <div class='col-lg-6'>
                <h4>Outputs</h4>
                <p>
                    The output is an array of objects. Please refer to the table below for the structure
                    of the single object.
                </p>
                <table class='table table-bordered table-responsive table-striped table-hover table-condensed'>
                    {{> table_headers}}
                    <tbody>
                        <tr>
                            <td><kbd>code</kbd></td>
                            <td>Group code.</td>
                            <td><var>Q</var></td>
                        </tr>
                        <tr>
                            <td><kbd>label</kbd></td>
                            <td>Group label.</td>
                            <td><var>Production</var></td>
                        </tr>
                        <tr>
                            <td><kbd>order</kbd></td>
                            <td>Order of the group.</td>
                            <td><var>110</var></td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class='col-lg-6'>
                <h4>Output Example</h4>
                {{> home_groups_out}}
            </div>
        </div>
    </div>
</script>

